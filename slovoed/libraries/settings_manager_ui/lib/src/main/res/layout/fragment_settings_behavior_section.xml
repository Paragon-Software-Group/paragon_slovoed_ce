    <TextView
            style="@style/SettingsTitle"
            android:text="@string/settings_manager_ui_behavior"/>

    <androidx.appcompat.widget.SwitchCompat
            style="@style/SettingsSwitch"
            android:id="@+id/show_keyboard_for_search_switch"
