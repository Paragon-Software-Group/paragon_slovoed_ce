<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/fc_blue"/>
            <size
                android:width="@dimen/tick_icon_size"
                android:height="@dimen/tick_icon_size"/>
        </shape>
    </item>
    <item android:drawable="@drawable/ic_tick"
          android:left="4dp"
          android:right="4dp"
          android:top="4dp"
          android:bottom="4dp"
          android:gravity="center"/>
</layer-list>
